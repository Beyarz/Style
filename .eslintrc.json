{
  "env": {
    "browser": true,
    "es2020": true
  },
  "extends": [
    "standard"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "no-extra-semi": "error",
    "no-mixed-spaces-and-tabs": "error",
    "no-unused-vars": "off"
  }
}
